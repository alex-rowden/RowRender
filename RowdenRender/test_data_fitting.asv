data = getAVWData();
step = .00001;
x_range = min(data.x):step:max(data.x);
y_range = min(data.y):step:max(data.y);
z_range = min(data.floor):step:max(data.floor);
all_umd_routers = data(strcmp(data.wifi_name, "umd"), :);
macs = unique(all_umd_routers.mac(:));

for mac_index = 1:size(macs,1) 
    [mu, axis, r, min_coord, max_coord] = fitEllipsoid(take_a_mac, data);
    x_range = (min_coord(1) - .001):.00001:(max_coord(1) + .001 );
    y_range = (min_coord(2) - .001):.00001:(max_coord(2) + .001 );
    z_range = (min_coord(3) - .001):.00001:(max_coord(3) + .001 );
    v = plotEllipsoid(mu, axis, r, x_range, y_range, z_range);
end
write3Dtex("umd_routers.tex", v);
